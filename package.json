{
  "name": "fluidms",
  "version": "0.1.1",
  "description": "A font-size system based on Modular Scale and Fluid Typography built with Sass and CSS custom properties.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/csshugs/FluidMS.git"
  },
  "scripts": {
    "server": "npx http-server test/",
    "sass:watch": "npx node-sass test/style.scss test/style.css --watch"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "CSS",
    "Modular Scale",
    "Sass",
    "Fluid Typography",
    "Custom properties"
  ],
  "author": "Dennis Heib√ºlt",
