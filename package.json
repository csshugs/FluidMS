  "bugs": {
    "url": "https://github.com/csshugs/FluidMS/issues"
  },
  "homepage": "https://github.com/csshugs/FluidMS#readme",
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "http-server": "^0.11.1",
    "husky": "^3.0.5",
    "node-sass": "^4.12.0"
  }
}
